#include "database.h"

static void database::set() {
  try {
    connection *c = new connection("dbname=xxx user=postgres password = passw0rd");
    if (!c->is_open()) {
      cerr << "Cannot open database" << endl;
    }
  } catch(const std::exception &e) {
    cerr << e.what() << std::endl;
  }
     
}